				<key>script</key>
				<string>#!/bin/bash

# Get the text from the clipboard
input_text=$(pbpaste)

# Use regular expression to find all numbers in the text and calculate the sum
total=0
while [[ $input_text =~ ([0-9]+) ]]; do
  total=$((total + ${BASH_REMATCH[1]}))
  input_text=${input_text#*"${BASH_REMATCH[0]}"}
done

# Print the result to Alfred
echo "$total" | tr -d \\n | pbcopy | pbpaste</string>
